{
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended",
    "plugin:security/recommended",
    "prettier"
  ],
  "plugins": ["security", "prettier"],
  "parserOptions": { "ecmaVersion": 2017 },
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "no-console": "warn",
    "prettier/prettier": "error"
  }
}
